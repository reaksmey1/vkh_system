<div class="row">
	<div class="col-md-1"></div>
	<div class="col-md-6">
		<div class="nav">
		<%= form_tag historical_reports_path, :method => 'get', :class => "form-inline" do %>
			<div class="navbar-nav navbar-left" style="margin-bottom: 10px;">
				<%= text_field_tag :search, params[:search], class: "form-control", placeholder: "Search ..." %>
				<%= submit_tag "Search", :class => "btn btn-default" %>
			</div>
		<% end %>
  	</div>

	  <div class="panel panel-default">
			<table class="table table-striped table-hover" id="sell_report">
				<th style="width: 25%"> លេខឡាន </th>
				<th style="width: 25%"> ថ្ងៃចូល </th>
				<th style="width: 25%"> គីឡូ / ម៉ាយ </th>
				<th style="width: 25%"> </th>

				<% @car_histories.each do |el| %>
				<tr>
				<td> <%= el.car.plate_number %> </td>
				<td> <%= el.entry_date.strftime("%d/%m/%Y") %> </td>
				<td> <%= el.kilo_metre %> </td>
				<td>
					<div>
						<span style = "margin-right: 5px;">
							<%= link_to "", historical_report_path(el), class: "glyphicon glyphicon-zoom-in" %>
						</span>
					</div>
				</td>
			</tr>
			<% end %>
			</table>
		</div>
	</div>
</div>