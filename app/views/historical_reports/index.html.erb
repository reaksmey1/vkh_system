<table class="table table-striped table-hover" id="sell_report">
	<th style="width: 15%"> លេខឡាន </th>
	<th style="width: 15%"> ថ្ងៃចូល </th>
	<th style="width: 15%"> គីឡូ / ម៉ាយ </th>
	<th style="width: 10%"> </th>
	<% @car_histories.each do |el| %>
		<tr>
		<td> <%= el.car.plate_number %> </td>
		<td> <%= el.entry_date %> </td>
		<td> <%= el.kilo_metre %> </td>
		<td>
			<div>
				<span style = "margin-right: 10px;">
					<%= link_to "", edit_historical_report_path(el), class: "glyphicon glyphicon-pencil" %>
				</span>
				<span style = "margin-right: 5px;">
					<%= link_to "", historical_reports_path, class: "glyphicon glyphicon-zoom-in" %>
				</span>
			</div>
		</td>
	</tr>
	<% end %>
</table>